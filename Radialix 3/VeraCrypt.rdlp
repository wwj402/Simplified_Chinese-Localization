                    <Src CodePage="65001">SYS_ENCRYPTION_PRETEST_RESULT_TITLE</Src>
                    <Trg Created="2015-08-18T15-22-45"/>
                  </Vl>
                </Cl>
                <Vl Name="text" Class="XML_TEXT_NODE">
                  <Src CodePage="65001">预测试已经完成</Src>
